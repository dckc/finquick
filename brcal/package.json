{
  "scripts": {
    "select": "node -r esm src/main.js --db >../,uncat.json",
    "sync": "node -r esm src/main.js --sync",
    "sync:etherscan": "node -r esm src/etherscan.js",
    "sync:uniswap": "node -r esm src/unisync.js",
    "lint:types": "tsc -p jsconfig.json",
    "build": "exit 0",
    "test": "exit 1",
    "pretty-fix": "prettier --write '**/*.js'",
    "pretty-check": "prettier --check '**/*.js'",
    "lint-fix": "yarn lint:eslint --fix",
    "lint-check": "yarn lint",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:eslint": "eslint '**/*.js'"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "fast-json-patch": "^3.0.0-1",
    "follow-redirects": "^1.13.0",
    "ical": "^0.8.0",
    "mysql": "^2.18.1",
    "require-text": "^0.0.1"
  },
  "devDependencies": {
    "@types/follow-redirects": "^1.13.0",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.14.7",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-jessie": "^0.0.4",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsdoc": "^30.4.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "prettier": "^1.13.0",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
